package dataofstudents;

public class Student {
	int id;
	String Name ;
	static String CollegeName="Raghu Enigineering college";
	
	Student(int r , String n){
		id = r;
		Name = n ;
		
	}
	void display() {
		System.out.println(id+" "+Name+" "+CollegeName);
	}
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Student a1 = new Student( 111 , "Ganesh");
		Student a2 = new Student( 121 , "praveen");
		a1.display();
		a2.display();
	}

}
