package javaprogramns;


class Anil{
	String name =" Ganesh";
}
 class Praveen extends Anil{
	 String name =" Prasanth";
 void PrrintName() {
	 System.out.println(name);
	 System.out.println(super.name);// This is the example for super keyword which is used as reference  to immediate parenrt class instsnce variable
	 
 }
 }
public class Super {
	public static void main(String[] args) {
		Praveen abc = new Praveen()	;
		abc.PrrintName();
	}
}


