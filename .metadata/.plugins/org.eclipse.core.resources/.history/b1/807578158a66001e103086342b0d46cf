

class Vehicle{
	void Name() {
		System.out.println("Vehicle Name is Honda");
	}
}
class Car extends Vehicle{
	void Run() {
		System.out.println("Car is runnig");
	}
	
}

public class Main{
	public static void main(String[] args) {
		System.out.println("This is example of inheritance");
		 Car car = new Vehicle();
		 car.Run();
		 car.Name();
	}
}


